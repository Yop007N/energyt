{
  "name": "energy-monitoring-dashboard",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Dashboard moderno para monitoreo y análisis de consumo energético con React, TypeScript y Vite",
  "keywords": ["energy", "monitoring", "dashboard", "analytics", "typescript", "react", "vite"],
  "author": "Enrique Bobadilla",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "build:analyze": "npm run build && npx vite-bundle-analyzer dist",
    "serve": "serve -s dist -l 5173"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.20",
    "axios": "^1.7.2",
    "chart.js": "^4.4.3",
    "date-fns": "^3.6.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-datepicker": "^7.1.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-query": "^3.39.3",
    "react-resizable": "^3.0.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-resizable": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
